<h1>All users</h1>
<%= paginate @users %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: 50 %>
      <%= link_to user.name, user %>
      <span><%= link_to "Make friends", "#", class: "label label-success" %></span>
    </li>
  <% end %>
</ul>

<%= paginate @users %>
